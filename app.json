{
  "expo": {
    "name": "MinhaEscala",
    "slug": "MinhaEscala",
    "version": "1.0.0",
    "userInterfaceStyle": "automatic",
    "scheme": "minhaescala",
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      [
        "expo-notifications",
        {
          "icon": "./src/assets/icon.png",
          "color": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.james2.MinhaEscala",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.james2.MinhaEscala"
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "7c009e7e-fb93-4552-a77e-76cca1477caf"
      }
    },
    "runtimeVersion": "1.0.0",
    "updates": {
      "url": "https://u.expo.dev/7c009e7e-fb93-4552-a77e-76cca1477caf"
    }
  }
}